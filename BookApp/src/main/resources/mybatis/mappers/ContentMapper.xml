<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.benxinm.bookapp.mappers.ContentMapper">
    <select id="getContentById" resultType="com.benxinm.bookapp.entity.Content">
        select content_id id,tx tx from t_content where content_id=#{id}
    </select>
</mapper>